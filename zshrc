test -e $HOME/myzsh/theme.zsh && source $HOME/myzsh/theme.zsh
test -e $HOME/myzsh/functions.zsh && source $HOME/myzsh/functions.zsh
test -e $HOME/myzsh/init.zsh && source $HOME/myzsh/init.zsh
